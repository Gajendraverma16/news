                    <AlertDialogFooter>
                      <AlertDialogCancel onClick={() => setLogoutConfrm(false)}>{translate('noLbl')}</AlertDialogCancel>
                      <AlertDialogAction onClick={() => handleSignOut('logoutAcc')}>{`${translate('yesLbl')},${translate('logout')}`}</AlertDialogAction>
