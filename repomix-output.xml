        language_id: currentLanguage?.id
      });

      if (!data?.error) {
        if (data?.data?.[0]?.bookmark == 0) {
          setBookmark(false);
        } else {
          setBookmark(true);
        }

        if (data?.data?.[0]?.like === 0) {
          setLike(false);
        } else {
          setLike(true);
        }

        setData(data)
        setLoading(false)
      }
      else {
        console.log('error =>', data?.message)
        setData([])
        setLoading(false)
      }

    } catch (error) {
      console.error('Error:', error);
      setData([])
    } finally {
      setLoading(false)
    }
  };

  const getBreakingNewsDetails = async () => {
    try {
      setLoading(true)
      const { data } = await getBreaingNewsApi.getBreakingNews({
        slug: slug,
        language_id: currentLanguage?.id
      });

      if (!data?.error) {
        setData(data)
        setLoading(false)
      }
      else {
