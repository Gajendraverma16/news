// 5. POLICY PAGES API
export const getNewsApi = {
    getNews: ({
        language_id,
        offset,
        limit,
        id,
        get_user_news,
        search, // {optional}
        category_id,
        category_slug,
        subcategory_id,
        subcategory_slug,
        slug,
        tag_id,
        tag_slug,
        latitude,
        longitude
    }) => {
        return Api.post(GET_NEWS, { language_id, offset, limit, id, get_user_news, search, category_id, category_slug, subcategory_id, subcategory_slug, slug, tag_id, tag_slug, latitude, longitude });
    },
}

